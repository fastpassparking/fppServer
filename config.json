{
  "development": {
    "serverPort": 3000,
    "dbPort": "localHost::272014/fppDb",
    "dbOptions": {
      "server": {
        "poolSize": 5,
        "socketOptions": {
          "keepAlive": 1
        }
      }
    },
    "errorHandlerOptions": {"dumpExceptions": true, "showStack": true}
  },
  "dev_server": {
    "serverPort": 80,
    "dbPort": "localHost::272014/fppDb",
    "dbOptions": {
      "server": {
        "poolSize": 5,
        "socketOptions": {
          "keepAlive": 1
        }
      }
    },
    "errorHandlerOptions": {"dumpExceptions": true, "showStack": true}
  },
  "production": {
    "serverPort": 3003,
    "dbPort": "localHost::272014/fppDb",
    "dbOptions": {
      "server": {
        "poolSize": 100,
        "socketOptions": {
          "keepAlive": 1
        }
      }
    },
    "errorHandlerOptions": {"dumpExceptions": false, "showStack": false}
  }
}
